{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "@open-wc",
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "class-methods-use-this": "warn",
    "no-unused-vars": "off",
    "wc/guard-super-call": "off",
    "@typescript-eslint/no-unused-vars": [
      "error"
    ],
    "import/no-unresolved": "off",
    "import/extensions": [
      "error",
      "always",
      {
        "ignorePackages": true
      }
    ]
  }
}